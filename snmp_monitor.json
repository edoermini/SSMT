    "attributes": {
     "name": "System uptime",
     "properties": {
      "shape": "chronograf-v2",
      "type": "single-stat",
      "queries": [
       {
        "text": "import \"math\"\n\nfrom(bucket: \"snmp\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"snmp\")\n  |> filter(fn: (r) => r[\"_field\"] == \"uptime\")\n  |> map(fn: (r) => ({ r with _value: \n      string(v: math.round(x: float(v: r._value) / 86400.0 / 100.0 )) + \"d \" +\n      string(v: math.round(x: ((float(v: r._value) / 100.0) % 86400.0) / 3600.0 )) + \"h \" +\n      string(v: math.round(x: ((float(v: r._value) / 100.0) % 3600.0 ) / 60.0)) + \"m \" +\n      string(v: math.round(x: ((float(v: r._value) / 100.0) % 60.0))) + \"s \"\n    }))",
        "editMode": "advanced",
        "name": "",
        "builderConfig": {
